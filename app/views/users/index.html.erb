<script>
  (function() {
    var token = '<%= session[:api_token] %>';
    if (token !== '') {
      var a = token.split(' ');
      if (a.length === 2) {
        document.cookie = ('XNClient=' + a[0] + '; path=/');
        document.cookie = ('XNToken=' + a[1] + '; path=/');
      }
    }
    return true;
  })();
</script>
<%= render :partial => 'users/users_table' %>
